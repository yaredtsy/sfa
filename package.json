   "scripts": {
      "start": "nodemon src/index.ts",
      "seed": "ts-node seed.ts",
      "dev": "cross-env NODE_PATH=./src nodemon  src/index.ts",
      "migration": "ts-node ./node_modules/typeorm/cli.js migration:generate -n migrationName -p",
      "coverage": "cross-env NODE_PATH=./src jest --coverage",
      "test": "jest --watchAll",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "lint": "eslint src --ext .js,.jsx,.ts,.tsx .",
      "lint:fix": "eslint src --fix --ext .js,.jsx,.ts,.tsx .",
      "nibble": "eslint-nibble src --ext .js,.jsx,.ts,.tsx ."
   }
